{
    "name": "mcfunction",
    "displayName": "Marshal Command Code",
    "description": "Syntax Highlighting for Minecraft Command Code.",
    "version": "1.0.0",
    "publisher": "Nathan Liparski, Matthew 'Moesh' Dryden",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mcfunction",
                "aliases": ["Minecraft Function", "mcfunction"],
                "extensions": [".mcfunction"],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "mcc",
                "aliases": ["Minecraft Commands", "mcc"],
                "extensions": [".mcc"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mcfunction",
                "scopeName": "source.mcfunction",
                "path": "./syntaxes/mcfunction.tmLanguage"
            },
            {
                "language": "mcc",
                "scopeName": "source.mcc",
                "path": "./syntaxes/mcc.tmLanguage"
            }
        ]
    }
}